{
  "categories": [
    {
      "title": "## üöÄ Features",
      "labels": ["enhancement"]
    },
    {
      "title": "## üêõ Bug Fixes",
      "labels": ["bug"]
    }
  ],
  "groupBy": {
    "type": "labels",
    "groups": [
      {
        "title": "### SDDSComponents",
        "labels": ["SDDSComponents"]
      },
      {
        "title": "### SDDSIcons", 
        "labels": ["SDDSIcons"]
      },
      {
        "title": "### SDDSTheme",
        "labels": ["SDDSTheme"]
      },
      {
        "title": "### Core",
        "labels": ["Core"]
      }
    ]
  },
  "ignore_labels": [
    "changelog-skip"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "label_extractor": [
    {
      "pattern": "^(feat|feature)(\\(.+\\))?:",
      "target": "enhancement"
    },
    {
      "pattern": "^(fix|bug)(\\(.+\\))?:",
      "target": "bug"
    }
  ]
}
