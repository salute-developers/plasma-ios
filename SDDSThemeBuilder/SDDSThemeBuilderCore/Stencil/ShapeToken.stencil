import SwiftUI
import UIKit

public extension ShapeToken {
    {% for key, value in json %}
    static var {{ key }}: Self {
        Self(cornerRadius: {{ value.cornerRadius }})
    }
    {% endfor %}
}

public struct Shapes {
    {%- for key, value in json %}
    let {{ key }} = ShapeToken.{{ key }}
    {%- endfor %}
}
